Proceso sin_titulo
	Definir i Como Entero;
	
	// El con paso solo es necesario cuando quieres de 2 a más
	// Si quieres ir de uno en uno no es necesario el con paso
	Para i <- 1 hasta 10 Hacer
		Escribir "Valor i ->", i ;
	FinPara
	Para i <- 1 hasta 10 con paso 2 hacer
		Escribir "Valor i ->", i;
	FinPara
	
	Para i <- 0 Hasta 6 con Paso 2 Hacer
		Escribir "Valor de i - >", i;
	FinPara
	
FinProceso
